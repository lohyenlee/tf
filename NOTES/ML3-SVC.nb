Cell[TextData[{
 "\[OpenCurlyDoubleQuote]Training a support vector machine\
\[CloseCurlyDoubleQuote] means adjusting ",
 StyleBox["w",
  FontWeight->"Bold"],
 " and ",
 StyleBox["b",
  FontSlant->"Italic"],
 " to minimize ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["F", "SVC"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "80be654b-0c55-439f-b25f-162f1d8bd978"],
 ".  We may evaluate the gradient of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["F", "SVC"], TraditionalForm]],ExpressionUUID->
  "aa3849ec-b741-493e-a293-fc14ed4a92c3"],
 " analytically:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"\[PartialD]", 
      SubscriptBox["F", "SVC"]}], 
     RowBox[{"\[PartialD]", 
      SubscriptBox["w", "i"]}]], "=", 
    RowBox[{
     RowBox[{
      UnderoverscriptBox[
       StyleBox["\[Sum]",
        FontSize->24], 
       RowBox[{"n", "=", "1"}], "N"], 
      RowBox[{
       FractionBox[
        RowBox[{"\[PartialD]", 
         SubscriptBox["f", "SVC"]}], 
        RowBox[{"\[PartialD]", 
         SubscriptBox["u", 
          RowBox[{"n", "\[InvisibleSpace]"}]]}]], 
       FractionBox[
        RowBox[{"\[PartialD]", 
         SubscriptBox["u", 
          RowBox[{"n", "\[InvisibleSpace]"}]]}], 
        RowBox[{"\[PartialD]", 
         SubscriptBox["w", "i"]}]]}]}], "=", 
     RowBox[{
      UnderoverscriptBox[
       StyleBox["\[Sum]",
        FontSize->24], 
       RowBox[{"n", "=", "1"}], "N"], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"-", 
         SubscriptBox["y", "n"]}], ")"}], 
       RowBox[{"\[CapitalTheta]", "(", 
        RowBox[{"1", "-", 
         RowBox[{
          SubscriptBox["y", "n"], "\[InvisibleSpace]", 
          SubscriptBox["u", "n"]}]}], ")"}], 
       SubscriptBox["x", "i"]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "f0b405a5-3d4d-41dc-bae7-774c879c0cf1"],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"\[PartialD]", 
      SubscriptBox["F", "SVC"]}], 
     RowBox[{"\[PartialD]", "b"}]], "=", 
    RowBox[{
     RowBox[{
      UnderoverscriptBox[
       StyleBox["\[Sum]",
        FontSize->24], 
       RowBox[{"n", "=", "1"}], "N"], 
      RowBox[{
       FractionBox[
        RowBox[{"\[PartialD]", 
         SubscriptBox["f", "SVC"]}], 
        RowBox[{"\[PartialD]", 
         SubscriptBox["u", 
          RowBox[{"n", "\[InvisibleSpace]"}]]}]], 
       FractionBox[
        RowBox[{"\[PartialD]", 
         SubscriptBox["u", 
          RowBox[{"n", "\[InvisibleSpace]"}]]}], 
        RowBox[{"\[PartialD]", "b"}]]}]}], "=", 
     RowBox[{
      UnderoverscriptBox[
       StyleBox["\[Sum]",
        FontSize->24], 
       RowBox[{"n", "=", "1"}], "N"], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"-", 
         SubscriptBox["y", "n"]}], ")"}], 
       RowBox[{"\[CapitalTheta]", "(", 
        RowBox[{"1", "-", 
         RowBox[{
          SubscriptBox["y", "n"], "\[InvisibleSpace]", 
          SubscriptBox["u", "n"]}]}], ")"}]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a9d52af3-2d11-4cdb-bcfd-ad3ca6e5df06"],
 ".\nThis allows us to use gradient descent to minimize ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["F", "SVC"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "89e739a4-6f81-4e58-bf49-b071af7b89e8"],
 ". "
}], "Text",
