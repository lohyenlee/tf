Cell[TextData[{
 "Certain types of datasets cannot be fit satisfactorily by generalized \
linear models.  The simplest and most notorious example is the binary XOR \
function ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{
     SubscriptBox["x", "1"], "\[CirclePlus]", 
     SubscriptBox["x", "2"]}]}], TraditionalForm]],ExpressionUUID->
  "014f5ad7-f47b-4b40-a2c2-0461dc04625b"],
 ".  There is no combination of bias ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["w", "0"], TraditionalForm]],ExpressionUUID->
  "1f913c85-554e-478e-8094-bb147653755b"],
 " and classification vector ",
 StyleBox["w",
  FontWeight->"Bold"],
 " such that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["w", "0"], "+", 
     RowBox[{
      StyleBox["w",
       FontWeight->"Bold",
       FontSlant->"Plain"], "\[CenterDot]", 
      StyleBox["x",
       FontWeight->"Bold",
       FontSlant->"Plain"]}]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "1"], "\[CirclePlus]", 
      SubscriptBox["x", "2"]}], "\[Congruent]", 
     RowBox[{
      SubscriptBox["x", "1"], "+", 
      SubscriptBox["x", "2"], "-", 
      RowBox[{"2", "\[InvisibleSpace]", 
       SubscriptBox["x", "1"], "\[InvisibleSpace]", 
       SubscriptBox["x", "2"]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "4bd9f2d7-d37d-49ae-b2c8-663d3cc969b0"],
 " for all ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "1"], ",", 
    RowBox[{
     SubscriptBox["x", "2"], "\[Element]", 
     RowBox[{"{", 
      RowBox[{"0", ",", "1"}], "}"}]}]}], TraditionalForm]],ExpressionUUID->
  "bd5a1d65-6ce7-4470-8746-f2ed587c061e"],
 ".  The data set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     SubscriptBox[
      StyleBox["x",
       FontWeight->"Bold",
       FontSlant->"Plain"], "n"], ",", 
     SubscriptBox["y", "n"]}], "}"}], TraditionalForm]],ExpressionUUID->
  "ec052452-44c5-4152-8932-13a1359e80d4"],
 " is not linearly separable: \n\n\t",
 Cell[BoxData[
  FormBox[GridBox[{
     {
      SubscriptBox["x", "1"], 
      SubscriptBox["x", "2"], 
      RowBox[{
       SubscriptBox["w", "0"], "+", 
       RowBox[{
        StyleBox["w",
         FontWeight->"Bold",
         FontSlant->"Plain"], "\[CenterDot]", 
        StyleBox["x",
         FontWeight->"Bold",
         FontSlant->"Plain"]}]}], 
      RowBox[{"\[Kappa]", "(", 
       RowBox[{
        SubscriptBox["w", "0"], "+", 
        RowBox[{
         StyleBox["w",
          FontWeight->"Bold",
          FontSlant->"Plain"], "\[CenterDot]", 
         StyleBox["x",
          FontWeight->"Bold",
          FontSlant->"Plain"]}]}], ")"}], "y"},
     {"0", "0", 
      SubscriptBox["w", "0"], 
      RowBox[{"\[Kappa]", "\[InvisibleSpace]", "(", 
       SubscriptBox["w", "0"], ")"}], "0"},
     {"0", "1", 
      RowBox[{
       SubscriptBox["w", "0"], "+", 
       SubscriptBox["w", "1"]}], 
      RowBox[{"\[Kappa]", "\[InvisibleSpace]", "(", 
       RowBox[{
        SubscriptBox["w", "0"], "+", 
        SubscriptBox["w", "1"]}], ")"}], "1"},
     {"1", "0", 
      RowBox[{
       SubscriptBox["w", "0"], "+", 
       SubscriptBox["w", "2"]}], 
      RowBox[{"\[Kappa]", "\[InvisibleSpace]", "(", 
       RowBox[{
        SubscriptBox["w", "0"], "+", 
        SubscriptBox["w", "2"]}], ")"}], "1"},
     {"1", "1", 
      RowBox[{
       SubscriptBox["w", "0"], "+", 
       SubscriptBox["w", "1"], "+", 
       SubscriptBox["w", "2"]}], 
      RowBox[{"\[Kappa]", "\[InvisibleSpace]", "(", 
       RowBox[{
        SubscriptBox["w", "0"], "+", 
        SubscriptBox["w", "1"], "+", 
        SubscriptBox["w", "2"]}], ")"}], "0"}
    }], TraditionalForm]],ExpressionUUID->
  "380a0a28-7a31-46c9-ad22-54acc7076f09"]
}], "Text",
