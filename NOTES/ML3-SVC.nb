  3.832105489358295*^9}},ExpressionUUID->"957d5091-46b4-49c5-b0a9-\
37817d408250"],

Cell[TextData[{
 "L2-reg L1-loss SVC:\tMin ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["F", "~"], "(", 
     StyleBox["W",
      FontWeight->"Bold",
      FontSlant->"Plain"], ")"}], "=", 
    RowBox[{
     RowBox[{
      UnderoverscriptBox[
       StyleBox["\[Sum]",
        FontSize->24], 
       RowBox[{"n", "=", "1"}], "N"], 
      RowBox[{
       UnderoverscriptBox[
        StyleBox["\[Sum]",
         FontSize->24], 
        RowBox[{
         SuperscriptBox["n", "\[Prime]"], "=", "1"}], "N"], 
       RowBox[{
        FractionBox["1", "2"], "\[InvisibleSpace]", 
        SubscriptBox["y", "n"], 
        SubscriptBox["y", 
         SuperscriptBox["n", "\[Prime]"]], 
        SubsuperscriptBox[
         StyleBox["x",
          FontWeight->"Bold",
          FontSlant->"Plain"], "n", 
         StyleBox["T",
          FontSlant->"Plain"]], "\[InvisibleSpace]", 
        SubscriptBox[
         StyleBox["x",
          FontWeight->"Bold",
          FontSlant->"Plain"], 
         SuperscriptBox["n", "\[Prime]"]], "\[InvisibleSpace]", 
        SubscriptBox["W", "n"], 
        SubscriptBox["W", 
         SuperscriptBox["n", "\[Prime]"]]}]}]}], "-", 
     RowBox[{"\[Lambda]", 
      RowBox[{
       UnderoverscriptBox[
        StyleBox["\[Sum]",
         FontSize->24], 
        RowBox[{"n", "=", "1"}], "N"], 
       SubscriptBox["W", "n"]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "a366b2d0-3934-4bc1-ac46-f5ac6eeeac1a"],
 " subject to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"0", "\[LessEqual]", 
    SubscriptBox["W", "n"], "\[LessEqual]", "1"}], TraditionalForm]],
  ExpressionUUID->"5b8e08dc-2da9-4828-b331-ff1fc29454f5"],
 "\nL2-reg L2-loss SVC:\tMin ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["F", "~"], "(", 
     StyleBox["W",
      FontWeight->"Bold",
      FontSlant->"Plain"], ")"}], "=", 
    RowBox[{
     RowBox[{
      UnderoverscriptBox[
       StyleBox["\[Sum]",
        FontSize->24], 
       RowBox[{"n", "=", "1"}], "N"], 
      RowBox[{
       UnderoverscriptBox[
        StyleBox["\[Sum]",
         FontSize->24], 
        RowBox[{
         SuperscriptBox["n", "\[Prime]"], "=", "1"}], "N"], 
       RowBox[{
        FractionBox["1", "2"], "\[InvisibleSpace]", 
        SubscriptBox["y", "n"], 
        SubscriptBox["y", 
         SuperscriptBox["n", "\[Prime]"]], 
        SubsuperscriptBox[
         StyleBox["x",
          FontWeight->"Bold",
          FontSlant->"Plain"], "n", 
         StyleBox["T",
          FontSlant->"Plain"]], "\[InvisibleSpace]", 
        SubscriptBox[
         StyleBox["x",
          FontWeight->"Bold",
          FontSlant->"Plain"], 
         SuperscriptBox["n", "\[Prime]"]], "\[InvisibleSpace]", 
        SubscriptBox["W", "n"], 
        SubscriptBox["W", 
         SuperscriptBox["n", "\[Prime]"]]}]}]}], "+", 
     RowBox[{"\[Lambda]", 
      RowBox[{
       UnderoverscriptBox[
        StyleBox["\[Sum]",
         FontSize->24], 
        RowBox[{"n", "=", "1"}], "N"], 
       RowBox[{
        FractionBox["1", "4"], "\[InvisibleSpace]", 
        RowBox[{
         SubscriptBox["W", "n"], "(", 
         RowBox[{
          SubscriptBox["W", "n"], "-", 
          FractionBox["4", "\[Lambda]"]}], ")"}]}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"2fda5438-13c5-4876-8737-fd6c27e6f557"],
 " subject to ",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    RowBox[{"0", "\[LessEqual]", 
     SubscriptBox["W", "n"], "\[LessEqual]", "\[Infinity]"}]}], 
   TraditionalForm]],ExpressionUUID->"0c4b985b-0043-4996-8ddc-d83cb93851c5"],
 " \nL2-reg LR:\t\tMin ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["F", "~"], "(", 
     StyleBox["W",
      FontSlant->"Plain"], ")"}], "=", 
    RowBox[{
     RowBox[{
      UnderoverscriptBox[
       StyleBox["\[Sum]",
        FontSize->24], 
       RowBox[{"n", "=", "1"}], "N"], 
      RowBox[{
       UnderoverscriptBox[
        StyleBox["\[Sum]",
         FontSize->24], 
        RowBox[{
         SuperscriptBox["n", "\[Prime]"], "=", "1"}], "N"], 
       RowBox[{
        FractionBox["1", "2"], "\[InvisibleSpace]", 
        SubscriptBox["y", "n"], 
        SubscriptBox["y", 
         SuperscriptBox["n", "\[Prime]"]], 
        SubsuperscriptBox[
         StyleBox["x",
          FontWeight->"Bold",
          FontSlant->"Plain"], "n", 
         StyleBox["T",
          FontSlant->"Plain"]], "\[InvisibleSpace]", 
        SubscriptBox[
         StyleBox["x",
          FontWeight->"Bold",
          FontSlant->"Plain"], 
         SuperscriptBox["n", "\[Prime]"]], 
        SubscriptBox["W", "n"], 
        SubscriptBox["W", 
         SuperscriptBox["n", "\[Prime]"]]}]}]}], "+", 
     RowBox[{"\[Lambda]", 
      RowBox[{
       UnderoverscriptBox[
        StyleBox["\[Sum]",
         FontSize->24], 
        RowBox[{"n", "=", "1"}], "N"], 
       RowBox[{"[", 
        RowBox[{
         RowBox[{
          SubscriptBox["W", "n"], "ln", " ", 
          SubscriptBox["W", "n"]}], "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"1", "-", 
            SubscriptBox["W", "n"]}], ")"}], 
          RowBox[{"ln", "(", 
           RowBox[{"1", "-", 
            SubscriptBox["W", "n"]}], ")"}]}]}], "]"}]}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"ea5765e5-da9a-4bdc-8b97-5cc1f65f9838"],
 "\t subject to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"0", "\[LessEqual]", 
    SubscriptBox["W", "n"], "\[LessEqual]", "1"}], TraditionalForm]],
  ExpressionUUID->"87d546d7-f2f4-4d6a-aa5e-89b51b1ec8e9"],
 ".\nThe constraints may be written using infinite step functions if one \
prefers.   ",
 StyleBox["(Warning: I have not checked my translation from the LIBLINEAR \
docs carefully!)",
  FontColor->RGBColor[1, 0, 0]]
}], "Text",
