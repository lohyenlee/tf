 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"\[Beta]0", ",", "\[Beta]1", ",", "\[Beta]2"}], "}"}], "=", 
   RowBox[{"{", 
    RowBox[{"1", ",", "1.0", ",", "0.3"}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"linkFunc", "[", "bdotx_", "]"}], ":=", 
   RowBox[{"1", "/", 
    RowBox[{"(", 
     RowBox[{"1", "+", 
      RowBox[{"Exp", "[", 
       RowBox[{"-", "bdotx"}], "]"}]}], ")"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"meanFunc", "[", "xd_", "]"}], ":=", 
   RowBox[{"linkFunc", "[", 
    RowBox[{"\[Beta]0", "+", 
