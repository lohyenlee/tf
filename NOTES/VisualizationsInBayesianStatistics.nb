     FractionBox["1", 
      RowBox[{"1", "+", 
       RowBox[{"exp", "(", 
        RowBox[{"-", 
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["\[Beta]", "0"], "+", 
           RowBox[{
            SubscriptBox["\[Beta]", "1"], 
